<div class="content-header">
    <p>Add Category</p>
    <a href="dash#!/{{storeId}}/categories/add" class="btn pull-right">Add Product</a>
</div>


<form class="add-form" ng-submit="saveCategory()">
    <div class="form-item">
        <label>Category name</label>
        <input class="form-input" ng-model="category.name">
    </div>
    <button type="submit">Submit</button>
</form>